<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="287.0" prefWidth="521.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.gui.editSongController">
    <children>
        <Label layoutX="92.0" layoutY="31.0" text="Title" />
        <Label layoutX="92.0" layoutY="69.0" prefHeight="18.0" prefWidth="41.0" text="Artist" />
        <Label layoutX="92.0" layoutY="107.0" text="Category" />
        <Label layoutX="92.0" layoutY="149.0" prefHeight="18.0" prefWidth="42.0" text="Time" />
        <TextField fx:id="titleField" layoutX="158.0" layoutY="27.0"/>
        <TextField fx:id="artistField" layoutX="158.0" layoutY="65.0" />

        <TextField fx:id="timeField" layoutX="158.0" layoutY="145.0" prefHeight="26.0" prefWidth="197.0" />

        <Button fx:id="btnClose" layoutX="387.0" layoutY="233.0" mnemonicParsing="false" onAction="#closeWin" text="Cancel" />
        <Button fx:id="btnSave" layoutX="452.0" layoutY="233.0" mnemonicParsing="false" onAction="#editSong" text="Save" />
        <MenuButton fx:id="btnDrop" layoutX="158.0" layoutY="105.0" mnemonicParsing="false" text="MenuButton">
            <items>
                <MenuItem mnemonicParsing="false" text="Pop" onAction="#getPop" />
                <MenuItem mnemonicParsing="false" text="Hip hop" onAction="#getHop"/>
                <MenuItem mnemonicParsing="false" text="Rap" onAction="#getRap"/>
                <MenuItem mnemonicParsing="false" text="Rock" onAction="#getRock"/>
            </items>
        </MenuButton>
        <Button layoutX="283.0" layoutY="104.0" mnemonicParsing="false" text="More..." />

    </children>
</AnchorPane>
